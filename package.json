		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-terser": "^0.4.4",
		"docdash": "^2.0.2",
		"esdoc": "^1.1.0",
		"esdoc-standard-plugin": "^1.0.0",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"ntfile": "^1.0.0",
		"prettier": "^3.5.3",
		"rollup": "^4.40.2",
		"typescript": "^5.8.3"
	},
