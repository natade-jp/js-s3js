{
	"plugins": ["plugins/markdown", "plugins/summarize", "jsdoc-export-default-interop"],
	"recurseDepth": 10,
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.m?js$",
		"excludePattern": "\\.test\\.m?js$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"referenceTitle": "Lumi3D",
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"includeDate": false
		}
	},
	"opts": {
		"destination": "./docs",
		"template": "./node_modules/docdash",
		"tutorials": "./tutorials",
		"encoding": "utf8",
		"recurse": true
	},
	"docdash": {
		"menu": {
			"Github repo": {
				"href": "https://github.com/natade-jp/js-lumi3d",
				"class": "menu-item"
			}
		},
		"static": false,
		"sort": true,
		"sectionOrder": ["Tutorials", "Classes"],
		"search": false,
		"commonNav": true,
		"collapse": true,
		"wrap": false,
		"typedefs": true,
		"navLevel": 2,
		"private": false,
		"scripts": [],
		"ShortenTypes": true
	}
}
